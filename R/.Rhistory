diag_max = function(y, positive = TRUE){
.sum_diag = function(x, Q){
sum(abs(diag(Q[,x])))
}
.postiv_sign = function(x){
for (kk in 1:ncol(x)) {
if (x[kk,kk]<0) {
x[,kk] = x[,kk] * -1
}
}
x
}
K = ncol(y)
column_id = combinat::permn(K)
optim_id = which.max(lapply(column_id, .sum_diag, Q = y))
.postiv_sign(y[,column_id[[optim_id]]])
}
