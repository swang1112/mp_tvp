Q = diag_max(Q)
B_foo =  x$chol %*% Q
if(Reorder){
sign_mat = matrix(c(1, -1, 1, 1), nrow = 2, ncol = 2)
if(all(sign(B_foo[1:2,1:2])== sign_mat)){
B_foo[,c(1,2)] = B_foo[,c(2,1)]
B_foo[, 2] = B_foo[, 2]*-1
}
}
list('Bmat' = .postiv_sign(B_foo),
'Amat' = x$A_hat )
}
